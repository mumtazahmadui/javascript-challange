let list = [1,2,1,4,7,2,3]
for(let i=0; i<list.length-1; i++) {
  for(let y=0; y<list.length-1-i; y++) {
    if(list[y] === list[y+1]) {
      list.splice(y, 1);
    }
  if(list[y]>list[y+1]) {
      let aa= y+1;
      var temp = list[y+1];
      list[y+1]=list[y];
      list[y] = temp;   
    }
  }
  }
  list
